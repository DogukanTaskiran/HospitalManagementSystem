@model List<RadiologicalReport>
<h1>Radiological Report</h1>
<a class="btn btn-primary"asp-action="Profile" asp-controller="Patient">Back</a>
<div class="form-group">
    <div class="col-md-12">
        &nbsp;
    </div>
</div>
<table class="table table-striped">
    <thead>
        <tr>
            <th>RadiologicalReportID</th>
            <th>Description</th>
            <th>Name</th>
            <th>Path</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var rr in @Model)
        {
            <tr>
                <td>@rr.RadiologicalReportID</td>
                <td>@rr.RrDescription</td>
                <td>@rr.filename</td>
                <td>@rr.filepath</td>
                
            </tr>
        }
    </tbody>
</table>