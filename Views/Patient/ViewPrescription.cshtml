@model List<Prescription>
<h1>Prescriptions</h1>
<a class="btn btn-primary"asp-action="Profile" asp-controller="Patient">Back</a>
<div class="form-group">
    <div class="col-md-12">
        &nbsp;
    </div>
</div>

<table class="table table-striped">
    <thead>
        <tr>
            <th>PrescriptionID</th>
            <th>Name</th>
            <th>Path</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var pres in @Model)
        {
            <tr>
                <td>@pres.PrescriptionID</td>
                <td>@pres.filename</td>
                <td>@pres.filepath</td>
                <td>@pres.PrescriptionDate</td>
                <td><a class="btn btn-success"href="~/uploads/@pres.filepath" download>Download</a></td>
                
            </tr>
        }
    </tbody>
</table>
